# .gitignore for Django projects

# Ignore virtual environment
venv/

# Ignore database files
*.sqlite3
*.db

# Ignore static files collected by collectstatic
staticfiles/

# Ignore media files
media/

# Ignore secret key
*.secret_key

# Ignore local settings
local_settings.py

# Ignore temporary files
*.pyc
*.pyo
__pycache__/

# Ignore logs
*.log


# Ignore IDE files and directories
.idea/
.vscode/
*.pyc

# Ignore migration files (only if you're not using them for version control)
**/migrations/*.py
**/migrations/*.pyc

# dependencies
/node_modules
/.pnp
.pnp.js

# testing
/coverage

# production
/build

# misc
.DS_Store
.env.local
.env.development.local
.env.test.local
.env.production.local

npm-debug.log*
yarn-debug.log*
yarn-error.log*
*.svg
*.png
# icon
*.icon
*.ico
# env
*.env

